  .bottom-text {
font-size: 16px;
font-weight: 600;
line-height: 24px;
color: #AAAAAA;
  }
  @media (max-width: 768px) {
    .main-text {
      width: 70%;
    }
    .text-wrap {
      flex-direction: column;
    }
  }
</style>
