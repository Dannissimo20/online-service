    color: #fff;
    font-size: 16px;
    font-weight: 700;
    line-height: 23.47px;
    border-radius: 8px;
    border: 1px solid #0000001A;
  }
  .top {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    gap: 16px;
    flex-grow: 1;
  }
  .bottom {

    margin-top: auto;

  }
  .text-wrap {
    display: flex;
    align-items: center;
  }
  .main-text {
  font-size: 20px;
  font-weight: 600;
  line-height: 24px;
  letter-spacing: 0.1px;
  text-align: center;
  }
  .text {
    color: #767676;
font-size: 14px;
line-height: 16px;
letter-spacing: 0.10000000149011612px;
text-align: center;

